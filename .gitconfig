[user]
name = chollasit-dev
email = 118034127+chollasit-dev@users.noreply.github.com

[credential]
helper = cache --timeout=3600

[init]
defaultBranch = main

[merge]
autostash = true

[pull]
default = current
rebase = true

[push]
autoSetupRemote = true
default = simple # push to matching branch name by default

[rerere]
enabled = true # reused, recorded, resolution to solve conflict

[rebase]
autostash = true
autosquash = true

[oh-my-zsh]
git-commit-alias = 95ef2516697aa764d1d4bb93ad3490584cc118ec

[alias]
build = "!a() {\nlocal _scope _attention _message\nwhile [ $# -ne 0 ]; do\ncase $1 in\n  -s | --scope )\n    if [ -z $2 ]; then\n      echo \"Missing scope!\"\n      return 1\n    fi\n    _scope=\"$2\"\n    shift 2\n    ;;\n  -a | --attention )\n    _attention=\"!\"\n    shift 1\n    ;;\n  * )\n    _message=\"${_message} $1\"\n    shift 1\n    ;;\nesac\ndone\ngit commit -m \"build${_scope:+(${_scope})}${_attention}:${_message}\"\n}; a"
